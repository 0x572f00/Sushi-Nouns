{
  "name": "sushi-nouns-playground",
  "version": "0.1.0",
  "author": "Sebastien",
  "license": "CC0-1.0",
  "keywords": [
    "sushi",
    "nouns",
    "nounsdao",
    "playground"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "encode": "ts-node -P tsconfig.scripts.json src/scripts/encode.ts",
    "build": "yarn run clean-dist && webpack --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "yarn run start-dev",
    "start-dev": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@nouns/sdk": "^0.4.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.6",
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^20.8.6",
    "@types/pngjs": "^6.0.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "express": "^4.18.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "pngjs": "^6.0.0",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-refresh": "^0.13.0",
    "rimraf": "^3.0.2",
    "sass": "^1.45.2",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@nouns/assets": "^0.4.2",
    "@nouns/contracts": "^0.1.3",
    "assert": "^2.0.0",
    "bootstrap": "^5.3.2",
    "chai": "^4.3.7",
    "ethers": "^5.7.2",
    "fs": "^0.0.1-security",
    "mocha": "^10.1.0",
    "patch-package": "^8.0.0",
    "path": "^0.12.7",
    "pngjs": "^6.0.0",
    "postinstall-postinstall": "^2.1.0",
    "process": "^0.11.10",
    "react-bootstrap": "^2.9.0",
    "stream": "^0.0.2"
  }
}
